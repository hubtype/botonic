{
  "scripts": {
    "build": "rm -rf lib && tsc",
    "build_with_tests": "tsc -b tests/tsconfig.json",
    "test": "jest",
    "lint": "node_modules/.bin/eslint_d --fix --cache --quiet 'src/**/*.ts*' 'tests/**/*.ts*'",
    "lint_slow": "node_modules/.bin/eslint_d -c .eslintrc_slow.js --fix --cache --quiet 'src/**/*.ts*' 'tests/**/*.ts*'",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src tests",
    "postversion": "git push && git push --tags"
  },
  "name": "@botonic/plugin-dynamodb",
  "version": "0.9.12",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubtype/botonic.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@babel/runtime": "^7.5.5",
    "aws-sdk": "^2.548.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^22.11.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint_d": "^8.0.0",
    "jest": "^25.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-mockito": "^2.4.2",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=8.10.0"
  }
