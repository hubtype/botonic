{
  "extends": "../../tsconfig.base.json",
  "include": ["src/"],
  "moduleFileExtensions": ["js", "jsx", "json", "tsx"],
  "compilerOptions": {
    "sourceRoot": "src",
    "baseUrl": ".",
    "outDir": "./lib/cjs",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "nodenext",
    "jsx": "react-jsx",
    "lib": ["ES2021.String"]
  }
}
