{
  "name": "@botonic/cli",
  "description": "Build Chatbots Using React",
  "version": "0.40.0",
  "license": "MIT",
  "bin": {
    "botonic": "./bin/run.js"
  },
  "scripts": {
    "prepublishOnly": "npm i && npm run build",
    "build": "shx rm -rf lib && tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "lint": "npm run lint_core -- --fix",
    "lint_core": "../../node_modules/.bin/eslint --quiet",
    "test": "../../node_modules/.bin/jest --coverage",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.6",
    "@oclif/core": "^4.5.4",
    "@oclif/plugin-help": "^6.2.33",
    "@oclif/plugin-plugins": "^5.4.48",
    "axios": "^1.12.2",
    "fs-extra": "^11.3.2",
    "ora": "^9.0.0",
    "picocolors": "^1.1.1",
    "zip-a-folder": "^3.1.9"
  },
  "devDependencies": {
    "@eslint/compat": "^1.0.0",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "eslint-config-oclif": "^6.0.0",
    "eslint-config-prettier": "^10.1.8",
    "oclif": "^4.22.27",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.19.0",
    "npm": ">=10.0.0"
  },
  "bugs": "https://github.com/hubtype/botonic/issues",
  "files": [
    "./bin",
    "./lib",
    "./lib",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/hubtype/botonic",
  "main": "lib/index.js",
  "type": "module",
  "oclif": {
    "bin": "botonic",
    "dirname": "botonic",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "hubtype/botonic",
  "types": "lib/index.d.ts",
  "keywords": [
    "bot-framework",
    "chatbot",
    "cli",
    "conversational-app",
    "conversational-ui",
    "oclif",
    "typescript"
  ]
}